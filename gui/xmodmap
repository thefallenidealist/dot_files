! cursor, delete cluster													{{{
!------------------------------------------------------------------------------
! cursor keys -----------------------------------------------------------------
keycode  98 = Up NoSymbol uparrow
keycode 100 = Left NoSymbol leftarrow
keycode 102 = Right NoSymbol rightarrow
keycode 104 = Down NoSymbol downarrow
! delete/home/end cluster	---------------------------------------------------
!keycode 107 = Delete BackSpace Delete
!keycode  97 = Home NoSymbol Home
!keycode 103 = End NoSymbol End
! PageUp
!keycode  99 = Prior NoSymbol Prior
! PageDown
!keycode 105 = Next NoSymbol Next
!keycode 106 = Insert NoSymbol Insert
!---------------------------------------------------------------------------}}}
! ThinkPad keys																{{{
!------------------------------------------------------------------------------
! mute speaker
keycode 140 = XF86AudioMute NoSymbol XF86AudioMute
! Fn key, can't work as normal key nor as modifier
keycode 150 = NoSymbol NoSymbol
keycode 150 = F15 F15 F15

! This is already default
!! Fn + Left
!keycode 144 = XF86AudioPrev NoSymbol XF86AudioPrev
!! Fn + Right
!keycode 153 = XF86AudioNext NoSymbol XF86AudioNext
!! lower volume
!keycode 174 = XF86AudioLowerVolume NoSymbol XF86AudioLowerVolume
!! raise volume
!keycode 176 = XF86AudioRaiseVolume NoSymbol XF86AudioRaiseVolume
!
!keycode 162 = XF86AudioPlay XF86AudioPause XF86AudioPlay XF86AudioPause XF86AudioPlay XF86AudioPause
!keycode 164 = XF86AudioStop XF86Eject XF86AudioStop XF86Eject XF86AudioStop XF86Eject

