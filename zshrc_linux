# per OS specific config (linked to right ~/.zshrc_local via install_dot script)

PS1="$(print '%{\e[0;94m%}%M%{\e[0m%}%{\e[0;31m%}%#%{\e[0m%}') "	# use lighter blue color (94 instead of 34)
alias ls='ls -h --color'
alias lltr='ls -ltr'
alias gmake='make'
alias msg="grc cat /var/log/syslog"
alias w='wget'

alias unzip='/usr/bin/unzip'

